          "runserver",
          "0.0.0.0:8000"
        ],
        "django": true,
        "justMyCode": false
      },
      {
        "name": "Structure_Back: Shell",
        "type": "debugpy",
        "request": "launch",
        "program": "${workspaceFolder}/manage.py",
        "args": [
          "runscripts" // shell_plus, runscripts 
