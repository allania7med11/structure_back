        "type": "debugpy",
        "request": "launch",
        "program": "${workspaceFolder}/manage.py",
        "args": [
          "makemigrations"
        ],
        "django": true
      },
      {
        "name": "Structure_Back: migrate",
        "type": "debugpy",
        "request": "launch",
        "program": "${workspaceFolder}/manage.py",
        "args": [
          "migrate"
